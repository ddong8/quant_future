    "vuex": "^4.1.0",
    "element-plus": "^2.4.4",
    "echarts": "^5.4.3",
    "vue-echarts": "^6.6.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "@element-plus/icons-vue": "^2.1.0"
